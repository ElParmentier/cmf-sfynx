{% if is_granted('ROLE_USER') and not is_granted('ROLE_EDITOR') %}
    {% set auth_ws_params = getParameter('ws.auth') %}    	
    {% for domains in auth_ws_params.domains %}
    		<img src="{{ domains.url }}?ws_user_id={{ app.request.cookies.get('sfynx-ws-user-id') }}&ws_application={{ app.request.cookies.get('sfynx-ws-application-id') }}&ws_key={{ app.request.cookies.get('sfynx-ws-key') }}&expire=168000" width="0" border="0" style="position:absolute;display:none;" />
    {% endfor %}
{% endif %}    	